@model IEnumerable<Progra1_bases.Models.EstadoCuenta>

@{
    ViewData["Title"] = "ListarEstadosCuenta";
}

<h1>Estados de cuenta</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFinalizacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SaldoInicial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SaldoFinal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CantRetirosCajeroHumano)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CantRetirosCajeroAutomatico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SaldoMinimo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFinalizacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SaldoInicial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SaldoFinal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CantRetirosCajeroHumano)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CantRetirosCajeroAutomatico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SaldoMinimo)
            </td>
            <td>
                <a asp-action="Detalles" asp-route-id="@item.ID">Detalles</a>
            </td>
        </tr>
}
    </tbody>
</table>
